macro_accent_aigu: macro_accent_aigu {
compatible = "zmk,behavior-macro";
#binding-cells = <0>;
bindings
= <&macro_tap &kp SQT &kp E>;
};
macro_accent_grave: macro_accent_grave {
compatible = "zmk,behavior-macro";
#binding-cells = <0>;
bindings
= <&macro_tap &kp GRAVE &kp E>;
};
macro_u_grave: macro_u_grave {
compatible = "zmk,behavior-macro";
#binding-cells = <0>;
bindings
= <&macro_tap &kp GRAVE &kp U>;
};
macro_accent_cir: macro_accent_cir {
compatible = "zmk,behavior-macro";
#binding-cells = <0>;
bindings
= <&macro_tap &kp RA(N6) &kp E>;
};
macro_screenshot: macro_screenshot{
compatible = "zmk,behavior-macro";
label = "macro_screenshot";
#binding-cells = <0>;
bindings
= <&macro_press &kp LGUI &kp LCTRL>
, <&macro_tap &kp SPACE>
, <&macro_release &kp LGUI &kp LCTRL>;
};
macro_emoticonmenu: macro_emoticonmenu{
compatible = "zmk,behavior-macro";
label = "macro_emoticonmenu";
#binding-cells = <0>;
bindings
= <&macro_press &kp LGUI &kp LSHIFT>
, <&macro_tap &kp N4>
, <&macro_release &kp LGUI &kp LSHIFT>;
};
macro_quotes: macro_quotes{
compatible = "zmk,behavior-macro";
label = "macro_quotes";
#binding-cells = <0>;
bindings = <&kp SQT>,<&kp SPACE>,<&kp SQT>,<&kp SPACE>, <&kp LEFT>;
};
macro_dquotes: macro_dquotes{
compatible = "zmk,behavior-macro";
label = "macro_dquotes";
#binding-cells = <0>;
bindings = <&kp DQT>,<&kp SPACE>, <&kp DQT>,<&kp SPACE>, <&kp LEFT>;
};
macro_braces: macro_braces{
