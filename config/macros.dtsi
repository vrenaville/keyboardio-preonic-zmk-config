macro_kinesis: macro_kinesis{
compatible = "zmk,behavior-macro";
label = "macro_kinesis";
#binding-cells = <0>;
bindings = <&kp LS(K)>, <&kp INT6>, <&kp N>, <&kp E>, <&kp S>, <&kp I>, <&kp S>;
};
